gify(data));\n   115â†’    }\n   116â†’\n   117â†’    req.end();\n   118â†’  });\n   119â†’}\n   120â†’\n   121â†’/**\n   122â†’ * List available voices\n   123â†’ */\n   124â†’export async function listVoices(): Promise<ElevenLabsVoice[]> {\n   125â†’  const response = await makeRequest('GET', '/v1/voices');\n   126â†’  return response.voices || [];\n   127â†’}\n   128â†’\n   129â†’/**\n   130â†’ * Generate speech using ElevenLabs TTS\n   131â†’ */\n   132â†’export async function generateSpeech(options: GenerateOptions): Promise<Buffer> {\n   133â†’  const {\n   134â†’    text,\n   135â†’    voiceId,\n   136â†’    modelId = 'eleven_turbo_v2_5', // Fast, high-quality model\n   137â†’    stability = 0.5,\n   138â†’    similarityBoost = 0.75,\n   139â†’    style = 0.0,\n   140â†’    useSpeakerBoost = true,\n   141â†’  } = options;\n   142â†’\n   143â†’  console.log(`Generating speech with voice: ${voiceId}`);\n   144â†’  console.log(`Text length: ${text.length} characters`);\n   145â†’\n   146â†’  const apiPath = `/v1/text-to-speech/${voiceId}`;\n   147â†’  const requestData = {\n   148â†’    text,\n   149â†’    model_id: modelId,\n   150â†’    voice_settings: {\n   151â†’      stability,\n   152â†’      similarity_boost: similarityBoost,\n   153â†’      style,\n   154â†’      use_speaker_boost: useSpeakerBoost,\n   155â†’    },\n   156â†’  };\n   157â†’\n   158â†’  const stream = await makeRequest('POST', apiPath, requestData, true);\n   159â†’\n   160â†’  return new Promise((resolve, reject) => {\n   161â†’    const chunks: Buffer[] = [];\n   162â†’\n   163â†’    stream.on('data', (chunk: Buffer) => {\n   164â†’      chunks.push(chunk);\n   165â†’    });\n   166â†’\n   167â†’    stream.on('end', () => {\n   168â†’      const audioBuffer = Buffer.concat(chunks);\n   169â†’      console.log(`Generated audio size: ${(audioBuffer.length / 1024).toFixed(2)} KB`);\n   170â†’      resolve(audioBuffer);\n   171â†’    });\n   172â†’\n   173â†’    stream.on('error', reject);\n   174â†’  });\n   175â†’}\n   176â†’\n   177â†’/**\n   178â†’ * Generate reference audio for voice cloning\n   179â†’ * Creates multiple variations with different texts for better cloning results\n   180â†’ */\n   181â†’export async function generateReferenceAudio(\n   182â†’  voiceName: string,\n   183â†’  texts: string[],\n   184â†’  outputDir: string\n   185â†’): Promise<string[]> {\n   186â†’  const voiceId = VOICE_PRESETS[voiceName.toLowerCase()] || voiceName;\n   187â†’\n   188â†’  // Ensure output directory exists\n   189â†’  if (!fs.existsSync(outputDir)) {\n   190â†’    fs.mkdirSync(outputDir, { recursive: true });\n   191â†’  }\n   192â†’\n   193â†’  console.log(`Generating ${texts.length} reference audio files for voice: ${voiceName}`);\n   194â†’\n   195â†’  const outputPaths: string[] = [];\n   196â†’\n   197â†’  for (let i = 0; i < texts.length; i++) {\n   198â†’    const text = texts[i];\n   199â†’    const outputPath = path.join(outputDir, `${voiceName}-ref-${i + 1}.mp3`);\n   200â†’\n   201â†’    console.log(`\\n[${i + 1}/${texts.length}] Generating: ${text.substring(0, 50)}...`);\n   202â†’\n   203â†’    const audioBuffer = await generateSpeech({\n   204â†’      text,\n   205â†’      voiceId,\n   206â†’      stability: 0.6, // Slightly higher stability for reference audio\n   207â†’      similarityBoost: 0.8,\n   208â†’    });\n   209â†’\n   210â†’    fs.writeFileSync(outputPath, audioBuffer);\n   211â†’    console.log(`âœ“ Saved to: ${outputPath}`);\n   212â†’\n   213â†’    outputPaths.push(outputPath);\n   214â†’\n   215â†’    // Rate limiting: wait 1 second between requests\n   216â†’    if (i < texts.length - 1) {\n   217â†’      await new Promise((resolve) => setTimeout(resolve, 1000));\n   218â†’    }\n   219â†’  }\n   220â†’\n   221â†’  return outputPaths;\n   222â†’}\n   223â†’\n   224â†’/**\n   225â†’ * Default reference texts for voice cloning\n   226â†’ * These provide good phonetic coverage\n   227â†’ */\n   228â†’const DEFAULT_REFERENCE_TEXTS = [\n   229â†’  'The quick brown fox jumps over the lazy dog near the riverbank.',\n   230â†’  'She sells seashells by the seashore while watching the sunset.',\n   231â†’  'How much wood would a woodchuck chuck if a woodchuck could chuck wood?',\n   232â†’  'Peter Piper picked a peck of pickled peppers from the garden.',\n   233â†’  'Around the rugged rocks, the ragged rascal ran through the meadow.',\n   234â†’];\n   235â†’\n   236â†’/**\n   237â†’ * CLI entry point\n   238â†’ */\n   239â†’async function main() {\n   240â†’  const args = process.argv.slice(2);\n   241â†’  const command = args[0];\n   242â†’\n   243â†’  if (command === 'list-voices') {\n   244â†’    console.log('Fetching available voices...\\n');\n   245â†’    const voices = await listVoices();\n   246â†’\n   247â†’    console.log('Available voices:');\n   248â†’    voices.forEach((voice) => {\n   249â†’      console.log(`  ${voice.name} (${voice.voice_id})`);\n   250â†’      if (voice.description) {\n   251â†’        console.log(`    ${voice.description}`);\n   252â†’      }\n   253â†’    });\n   254â†’\n   255â†’    console.log('\\nPreset voice shortcuts:');\n   256â†’    Object.entries(VOICE_PRESETS).forEach(([name, id]) => {\n   257â†’      console.log(`  ${name}: ${id}`);\n   258â†’    });\n   259â†’  } else if (command === 'generate') {\n   260â†’    const textIndex = args.indexOf('--text');\n   261â†’    const voiceIndex = args.indexOf('--voice');\n   262â†’    const outputIndex = args.indexOf('--output');\n   263â†’\n   264â†’    if (textIndex === -1 || voiceIndex === -1 || outputIndex === -1) {\n   265â†’      console.error('Usage: generate --text \"Your text\" --voice rachel --output output.mp3');\n   266â†’      process.exit(1);\n   267â†’    }\n   268â†’\n   269â†’    const text = args[textIndex + 1];\n   270â†’    const voiceName = args[voiceIndex + 1];\n   271â†’    const outputPath = args[outputIndex + 1];\n   272â†’\n   273â†’    const voiceId = VOICE_PRESETS[voiceName.toLowerCase()] || voiceName;\n   274â†’\n   275â†’    const audioBuffer = await generateSpeech({ text, voiceId });\n   276â†’\n   277â†’    // Ensure output directory exists\n   278â†’    const outputDir = path.dirname(outputPath);\n   279â†’    if (!fs.existsSync(outputDir)) {\n   280â†’      fs.mkdirSync(outputDir, { recursive: true });\n   281â†’    }\n   282â†’\n   283â†’    fs.writeFileSync(outputPath, audioBuffer);\n   284â†’    console.log(`\\nâœ“ Audio saved to: ${outputPath}`);\n   285â†’  } else if (command === 'generate-reference') {\n   286â†’    const voiceIndex = args.indexOf('--voice');\n   287â†’    const outputIndex = args.indexOf('--output-dir');\n   288â†’\n   289â†’    if (voiceIndex === -1) {\n   290â†’      console.error('Usage: generate-reference --voice rachel [--output-dir path/to/output]');\n   291â†’      process.exit(1);\n   292â†’    }\n   293â†’\n   294â†’    const voiceName = args[voiceIndex + 1];\n   295â†’    const outputDir =\n   296â†’      outputIndex !== -1 ? args[outputIndex + 1] : path.join('public', 'assets', 'voices');\n   297â†’\n   298â†’    const paths = await generateReferenceAudio(voiceName, DEFAULT_REFERENCE_TEXTS, outputDir);\n   299â†’\n   300â†’    console.log('\\nâœ“ Reference audio generation complete!');\n   301â†’    console.log(`Generated ${paths.length} files in: ${outputDir}`);\n   302â†’    console.log('\\nThese files can now be used as reference audio for voice cloning.');\n   303â†’  } else {\n   304â†’    console.log('ElevenLabs Voice Generation - VC-002');\n   305â†’    console.log('');\n   306â†’    console.log('Commands:');\n   307â†’    console.log('  list-voices              List all available voices');\n   308â†’    console.log('  generate                 Generate single audio file');\n   309â†’    console.log('  generate-reference       Generate reference audio for voice cloning');\n   310â†’    console.log('');\n   311â†’    console.log('Examples:');\n   312â†’    console.log('  ts-node scripts/generate-voice-with-elevenlabs.ts list-voices');\n   313â†’    console.log(\n   314â†’      '  ts-node scripts/generate-voice-with-elevenlabs.ts generate --text \"Hello world\" --voice rachel --output audio.mp3'\n   315â†’    );\n   316â†’    console.log(\n   317â†’      '  ts-node scripts/generate-voice-with-elevenlabs.ts generate-reference --voice rachel'\n   318â†’    );\n   319â†’  }\n   320â†’}\n   321â†’\n   322â†’// Run CLI if executed directly\n   323â†’if (require.main === module) {\n   324â†’  main().catch((error) => {\n   325â†’    console.error('Error:', error.message);\n   326â†’    process.exit(1);\n   327â†’  });\n   328â†’}\n   329â†’\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"40bb03cb-ab28-452c-86a6-9dda2afa3a2d","uuid":"3eb1e210-24bb-4bd4-8b76-463e8d5c3161","tool_use_result":{"type":"text","file":{"filePath":"/Users/isaiahdupree/Documents/Software/ai-video-platform/scripts/generate-voice-with-elevenlabs.ts","content":"/**\n * ElevenLabs Reference Generation - VC-002\n * Generate voice reference audio using ElevenLabs TTS\n *\n * This script uses ElevenLabs TTS to generate high-quality reference audio\n * that can be used for voice cloning with IndexTTS or other models.\n *\n * Usage:\n *   ts-node scripts/generate-voice-with-elevenlabs.ts --text \"Your text\" --voice rachel --output reference.mp3\n *\n * Environment variables:\n *   ELEVENLABS_API_KEY: Your ElevenLabs API key\n */\n\nimport * as fs from 'fs';\nimport * as path from 'path';\nimport * as https from 'https';\nimport * as dotenv from 'dotenv';\n\ndotenv.config();\n\ninterface ElevenLabsVoice {\n  voice_id: string;\n  name: string;\n  category: string;\n  description?: string;\n}\n\ninterface GenerateOptions {\n  text: string;\n  voiceId: string;\n  modelId?: string;\n  stability?: number;\n  similarityBoost?: number;\n  style?: number;\n  useSpeakerBoost?: boolean;\n}\n\n/**\n * Popular ElevenLabs voice IDs\n * Get full list at: https://api.elevenlabs.io/v1/voices\n */\nconst VOICE_PRESETS: Record<string, string> = {\n  rachel: '21m00Tcm4TlvDq8ikWAM',\n  domi: 'AZnzlk1XvdvUeBnXmlld',\n  bella: 'EXAVITQu4vr4xnSDxMaL',\n  antoni: 'ErXwobaYiN019PkySvjV',\n  elli: 'MF3mGyEYCl7XYWbV9V6O',\n  josh: 'TxGEqnHWrfWFTfGW9XjX',\n  arnold: 'VR6AewLTigWG4xSOukaG',\n  adam: 'pNInz6obpgDQGcFmaJgB',\n  sam: 'yoZ06aMxZJJ28mfd3POQ',\n};\n\n/**\n * Get ElevenLabs API key from environment\n */\nfunction getApiKey(): string {\n  const apiKey = process.env.ELEVENLABS_API_KEY;\n  if (!apiKey) {\n    throw new Error('ELEVENLABS_API_KEY environment variable is required');\n  }\n  return apiKey;\n}\n\n/**\n * Make HTTPS request to ElevenLabs API\n */\nfunction makeRequest(\n  method: string,\n  apiPath: string,\n  data?: any,\n  isStream: boolean = false\n): Promise<any> {\n  return new Promise((resolve, reject) => {\n    const apiKey = getApiKey();\n\n    const options: https.RequestOptions = {\n      hostname: 'api.elevenlabs.io',\n      path: apiPath,\n      method: method,\n      headers: {\n        'xi-api-key': apiKey,\n        'Content-Type': 'application/json',\n      },\n    };\n\n    const req = https.request(options, (res) => {\n      if (isStream) {\n        // For audio streaming, return the response stream\n        resolve(res);\n      } else {\n        let body = '';\n        res.on('data', (chunk) => {\n          body += chunk;\n        });\n        res.on('end', () => {\n          if (res.statusCode !== 200) {\n            reject(new Error(`API error: ${res.statusCode} - ${body}`));\n          } else {\n            try {\n              resolve(JSON.parse(body));\n            } catch (e) {\n              resolve(body);\n   