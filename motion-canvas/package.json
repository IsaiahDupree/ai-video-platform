    "mc:two-pass": "npx tsx scripts/mc-two-pass.ts",
    "mc:server": "npx tsx scripts/mc-server.ts"
  },
  "dependencies": {
    "@motion-canvas/core": "^3.17.2",
    "@motion-canvas/2d": "^3.17.2",
    "@motion-canvas/ffmpeg": "^1.1.0"
  },
  "devDependencies": {
    "@motion-canvas/ui": "^3.17.2",
    "@motion-canvas/vite-plugin": "^3.17.2",
    "@types/node": "^20.0.0",
    "playwright": "^1.40.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.0.0"
  }
}