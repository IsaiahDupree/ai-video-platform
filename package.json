{
  "name": "ai-video-platform",
  "version": "1.0.0",
  "description": "AI video generation platform with voice cloning, static ad studio, and Apple product page builder",
  "private": true,
  "remotion": {
    "entryPoint": "src/Root.tsx"
  },
  "scripts": {
    "dev": "remotion studio",
    "build": "tsc",
    "render": "ts-node scripts/render-video.ts",
    "generate-tts": "ts-node scripts/generate-tts.ts",
    "generate-images": "ts-node scripts/generate-images.ts",
    "generate-voice": "ts-node scripts/generate-voice-with-elevenlabs.ts",
    "generate-batch-voiceover": "npx tsx scripts/generate-appkit-voiceover.ts",
    "generate-sfx": "npx tsx scripts/generate-sfx.ts",
